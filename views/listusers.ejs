<!doctype html>
<html>
<head>
    <title>User List - <%=shopName%></title>
    <link rel="stylesheet" type="text/css" href="/main.css" />
</head>
<body>
    <h1>User List</h1>
    <table>
        <thead>
            <tr>
                <th>Username</th>
                <th>First Name</th>
                <th>Last Name</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach(function(user) { %>
                <tr>
                    <td><%= user.Username %></td>
                    <td><%= user.firstname %></td>
                    <td><%= user.lastname %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>
